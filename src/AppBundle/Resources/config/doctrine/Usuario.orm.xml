<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Usuario" table="usuario">
    <indexes>
      <index name="complexionFK_idx" columns="complexion"/>
      <index name="dietaFK_idx" columns="dieta"/>
      <index name="rutinaFK_idx" columns="rutina"/>
      <index name="preferenciasFK_idx" columns="preferencias"/>
    </indexes>
    <id name="dni" type="integer" column="dni">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="username" type="string" column="username" length="45" nullable="true"/>
    <field name="sexo" type="boolean" column="sexo" nullable="true"/>
    <field name="edad" type="integer" column="edad" nullable="true"/>
    <field name="altura" type="integer" column="altura" nullable="true"/>
    <field name="password" type="string" column="password" length="64" nullable="false"/>
    <field name="nombre" type="string" column="nombre" length="45" nullable="true"/>
    <field name="apellido" type="string" column="apellido" length="45" nullable="true"/>
    <many-to-one field="rutina" target-entity="Rutina">
      <join-columns>
        <join-column name="rutina" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="complexion" target-entity="Complexion">
      <join-columns>
        <join-column name="complexion" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="preferencias" target-entity="GruposAlimenticios">
      <join-columns>
        <join-column name="preferencias" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="dieta" target-entity="Dieta">
      <join-columns>
        <join-column name="dieta" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-many field="idgrupo" target-entity="Grupo" mapped-by="idusuario"/>
    <many-to-many field="idcondiciones" target-entity="CondicionesDeSalud" mapped-by="idusuario"/>
  </entity>
</doctrine-mapping>
